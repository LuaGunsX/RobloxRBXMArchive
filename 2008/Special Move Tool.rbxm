function onSelected(mouse)
	mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;
	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
end

bin.Selected:connect(onSelected)

function onUnselected(mouse)
	local player = game.Players.LocalPlayer
	if player == nil then return end
	smo = player.Character:getChildren()
	for i = 1, #smo do
		if smo[i].Name == &quot;Smoke&quot; then
			smo[i]:Remove()
		elseif smo[i].Name == &quot;Torso&quot; then
			if smo[i]:findFirstChild(&quot;BodyAngularVelocity&quot;) ~= nil then
				smo[i].BodyAngularVelocity:Remove()
			end
		end
	end
	script:Clone().Parent = script.Parent
	script:Remove()
end

bin.Deselected:connect(onUnselected)

