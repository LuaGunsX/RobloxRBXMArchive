bin = script.Parent

function findNearestTorso(mouse, pos)
	local torso = nil
	dist = 100
	temp = mouse.Hit
	if (temp ~= nil) then
		if (temp.p - pos).magnitude &lt; dist then
			torso = mouse.Hit.p
			dist = (temp.p - pos).magnitude
		end
	end
	return torso
end

function teleportPlayer(pos)

	local player = game.Players.LocalPlayer
	if player == nil or player.Character == nil then return end

	local char = player.Character.Torso

	char.CFrame = CFrame.new(Vector3.new(pos.x, pos.y + 4, pos.z))

end


enabled = true
function onButton1Down(mouse)
	if not enabled then
		return
	end

	local player = game.Players.LocalPlayer
	if player == nil then return end
	local target = findNearestTorso(mouse, player.Character.Torso.Position)
	if target ~= nil then
		enabled = false
		local cf = mouse.Hit
		spin = Instance.new(&quot;BodyAngularVelocity&quot;)
		spin.maxTorque = Vector3.new(0,9e+009,0)
		spin.angularvelocity = Vector3.new(0,250,0)
		spin.Parent = player.Character.Torso
		player.Character.Humanoid.WalkSpeed = 0
		wait(0.2)
		smoke = game.Lighting.Smoke:Clone()
		smoke.Parent = player.Character
		smoke.CFrame = player.Character.Torso.CFrame
		wait(0.04)
		smoke.Transparency = 0.8
		wait(0.04)
		smoke.Transparency = 0.6
		wait(0.04)
		smoke.Transparency = 0.4
		wait(0.04)
		smoke.Transparency = 0.2
		wait(0.04)
		smoke.Transparency = 0
		wait(0.04)
		smoke.Transparency = 0.2
		wait(0.04)
		smoke.Transparency = 0.4
		wait(0.04)
		smoke.Transparency = 0.6
		wait(0.04)
		smoke.Transparency = 0.8
		wait(0.04)
		spin:Remove()
		smoke:Remove()
		teleportPlayer(cf.p)
		spin = Instance.new(&quot;BodyAngularVelocity&quot;)
		spin.maxTorque = Vector3.new(0,9e+009,0)
		spin.angularvelocity = Vector3.new(0,250,0)
		spin.Parent = player.Character.Torso
		smoke = game.Lighting.Smoke:Clone()
		smoke.Parent = player.Character
		smoke.CFrame = player.Character.Torso.CFrame
		wait(0.04)
		smoke.Transparency = 0.8
		wait(0.04)
		smoke.Transparency = 0.6
		wait(0.04)
		smoke.Transparency = 0.4
		wait(0.04)
		smoke.Transparency = 0.2
		wait(0.04)
		smoke.Transparency = 0
		wait(0.04)
		smoke.Transparency = 0.2
