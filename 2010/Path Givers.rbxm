s.CFrame = CFrame.new(s.Position,point)
r = Instance.new(&quot;ObjectValue&quot;)
r.Value = script.Parent.Parent.Parent
r.Parent = s
r.Name = &quot;creator&quot;
wait()
press(mouse)
end
end


