				<ProtectedString name="Source">model = script.Parent.Factory 
backup = model:clone() 

function Regen() 
model:remove() 
wait(1) 
model = backup:clone() 
model.Parent = script.Parent 
model:makeJoints() 
end 

while true do 
wait(1.5) 
if script.Parent.Factory.OwnerName.Value ~= &quot;&quot; then 
if not game.Players:FindFirstChild(script.Parent.Factory.OwnerName.Value) then
Regen() 

end 
end 
end </ProtectedString>
