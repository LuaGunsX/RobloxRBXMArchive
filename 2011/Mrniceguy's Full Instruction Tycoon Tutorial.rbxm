							<string name="Name">Script</string>
							<ProtectedString name="Source">money = script.Parent.Parent.Money -- Creates this code whenever you write &quot;money&quot; in this script.

function onTouched(hit) -- Does the following when a brick hits this brick.
&#9;if hit.Name == &quot;Copper&quot; then -- If the brick that touched this brick is named Copper, then do the following.
&#9;hit:remove() -- Remove the brick that touched this one.
&#9;money.Value = money.Value + 5 -- The value in the Money Value increases by 5.
&#9;end -- Ends above process.
&#9;if hit.Name == &quot;CleanedCopper&quot; then -- If the brick that touched this brick is named CleanedCopper, then do the following.
&#9;hit:remove() -- Remove the brick that touched this one.
&#9;money.Value = money.Value + 10 --The value in the Money Value increases by 10.
&#9;end -- Ends above process.
&#9;if hit.Name == &quot;Tin&quot; then -- If the brick that touched this brick is named Tin, then do the following.
&#9;hit:remove() -- Remove the brick that touched this one.
&#9;money.Value = money.Value + 15 -- The value in the Money Value increases by 15.
&#9;end -- Ends above process.
&#9;if hit.Name == &quot;CleanedTin&quot; then -- If the brick that touched this brick is named TinCopper, then do the following.
&#9;hit:remove() -- Remove the brick that touched this one.
&#9;money.Value = money.Value + 20 --The value in the Money Value increases by 20.
&#9;end -- Ends above process.
&#9;if hit.Name == &quot;Silver&quot; then -- If the brick that touched this brick is named Silver, then do the following.
&#9;hit:remove() -- Remove the brick that touched this one.
&#9;money.Value = money.Value + 25 -- The value in the Money Value increases by 25.
&#9;end -- Ends above process.
&#9;if hit.Name == &quot;CleanedSilver&quot; then -- If the brick that touched this brick is named CleanedSilver, then do the following.
&#9;hit:remove() -- Remove the brick that touched this one.
&#9;money.Value = money.Value + 30 --The value in the Money Value increases by 30.
&#9;end -- Ends above process.
&#9;if hit.Name == &quot;Gold&quot; then -- If the brick that touched this brick is named Gold, then do the following.
&#9;hit:remove() -- Remove the brick that touched this one.
&#9;money.Value = money.Value + 35 -- The value in the Money Value increases by 35.
&#9;end -- Ends above process.
&#9;if hit.Name == &quot;CleanedGold&quot; then -- If the brick that touched this brick is named CleanedGold, then do the following.
&#9;hit:remove() -- Remove the brick that touched this one.
&#9;money.Value = money.Value + 50 --The value in the Money Value increases by 50.
&#9;end -- Ends above process.
end -- Ends above process.

script.Parent.Touched:connect(onTouched) -- Connect this to all bricks that touch this one.</ProtectedString>
