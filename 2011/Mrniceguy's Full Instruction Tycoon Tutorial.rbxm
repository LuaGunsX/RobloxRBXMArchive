&#9;check = hit.Parent:FindFirstChild(&quot;Humanoid&quot;) -- check = person that hit the brick.

&#9;if check ~= nil then -- check to see that the player is REAL.
&#9;if hit.Parent.Name == owner.Value then -- checks to see if the player is the owner, if so do the following.
&#9;&#9;local user = game.Players:GetPlayerFromCharacter(hit.Parent) -- user = This player.
&#9;&#9;local stats = user:findFirstChild(&quot;leaderstats&quot;) -- stats = players stats.

&#9;&#9;if stats ~= nil then -- checks to see that stats are real.
&#9;&#9;&#9;local cash = stats:findFirstChild(&quot;Cash&quot;) -- cash = Player&apos;s Cash.
&#9;&#9;&#9;if cash.Value &gt; (Upgradecost-1) then -- if player&apos;s cash value = the upgrade cost then do the following.

&#9;&#9;&#9;&#9;cash.Value = cash.Value - Upgradecost -- subtract players cash by the upgrade cost.
&#9;&#9;&#9;&#9;upgradeStuff.Parent = script.Parent.Parent.Parent -- put the upgrate model into play.
&#9;&#9;&#9;&#9;script.Parent.Parent:remove() -- remove butten

&#9;&#9;&#9;end -- end process.
&#9;&#9;end -- end process.
&#9;end -- end process.
&#9;end -- end process.

&#9;ting = 0 -- set debounce to 0
&#9;end -- end process.
end -- end process.

script.Parent.Touched:connect(onTouched) -- connect the script to all who touch this butten.
