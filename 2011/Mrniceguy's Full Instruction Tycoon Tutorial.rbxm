							<Content name="LinkedSource"><null></null></Content>
							<string name="Name">GetCash</string>
							<ProtectedString name="Source">function onTouched(part) -- Do the following when someone touches this brick.
&#9;local h = part.Parent:findFirstChild(&quot;Humanoid&quot;) -- h = the person that touched this.
&#9;if (h~=nil) then -- If the person that touched this is a PERSON, then do the following.
&#9;&#9;local owner = script.Parent.Parent.Parent.OwnerName -- owner = whoever is the owner of this tycoon.
&#9;&#9;if h.Parent.Name == owner.Value then --If the one who pressed the button is the owner then do the following.
&#9;&#9;&#9;local thisplr = game.Players:findFirstChild(h.Parent.Name) -- thisplr = The owner&apos;s name.
&#9;&#9;&#9;if (thisplr~=nil) then -- if the owner&apos;s name is REAL then do the following.
&#9;&#9;&#9;&#9;local stats = thisplr:findFirstChild(&quot;leaderstats&quot;) -- stat = The owner&apos;s stats.
&#9;&#9;&#9;&#9;if (stats~=nil) then -- If his stats exist then do the following.
&#9;&#9;&#9;&#9;&#9;local score = stats:findFirstChild(&quot;Cash&quot;) -- score = The owners money.
&#9;&#9;&#9;&#9;&#9;if (score~=nil) then -- If his money exists then do the following.
&#9;&#9;&#9;&#9;&#9;&#9;score.Value = score.Value + script.Parent.Parent.Money.Value -- Adds the &quot;cash to collect&quot; brick&apos;s money to owners money.
&#9;&#9;&#9;&#9;&#9;&#9;script.Parent.Parent.Money.Value = 0 -- sets the &quot;cash to collect&quot; brick&apos;s money to 0.
&#9;&#9;&#9;&#9;&#9;end -- Ends processes.
&#9;&#9;&#9;&#9;end -- Ends processes.
&#9;&#9;&#9;end -- Ends processes.
&#9;&#9;end -- Ends processes.
&#9;end -- Ends processes.
end -- Ends processes.

script.Parent.Touched:connect(onTouched) -- Connect this script to the owner.
</ProtectedString>
